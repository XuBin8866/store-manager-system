<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xxbb.storemanagersystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.UserOperatorActivity"></activity>
        <activity android:name=".activity.UserInfoActivity"></activity>
        <activity android:name=".activity.UserListActivity"></activity>
        <activity android:name=".activity.UserInsertActivity"></activity>
        <activity android:name=".activity.GoodsListActivity"></activity>
        <activity android:name=".activity.RegisterActivity"></activity>
        <activity android:name=".activity.GoodsOperatorActivity"></activity>
        <activity android:name=".activity.GoodsInsertActivity" />


    </application>

</manifest>